Algoritmo modulo
	Definir num1, num2 Como Entero
	
	Escribir "Ingrese primer número"
	leer num1
	Escribir "Ingrese segundo número"
	Leer num2
	
	division <- num1 / num2
	modul<-num1 MOD num2
	
	Escribir "la division de ", num1, " y ", num2, " es ", division
	Escribir  "El módulo de ", num1, " y ", num2, " es ", modul
 FinAlgoritmo
