// Mostrar el resultado de la suma de tres números que se ingresen desde teclado.
Algoritmo Suma
	Escribir "Ingrese primer numero"
	Leer Num1
	Escribir "Ingrese segundo numero numero"
	Leer Num2
	Escribir "Ingrese tercer numero"
	Leer Num3
	
	
	Total = Num1 + Num2 + Num3
	Escribir "Total", Total
FinAlgoritmo
